{
    "name": "nids-cdd",
    "build": {
      "dockerfile": "Dockerfile",
      "context": ".."
    },
    "runArgs": [
      "--name=nids-cdd"
    ],
    "workspaceFolder": "/workspace",
    "mounts": [{
      "type": "bind",
      "source": "${localEnv:USERPROFILE}/traffic_data",
      "target": "/${containerWorkspaceFolder}/src/main/traffic_data"
    }]
}

